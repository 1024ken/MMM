<div class= star>
  <div id= <%= "like-link-#{blog.id}" %> >
    <% if current_user.likes.find_by(blog_id: blog.id) %>
      <%= link_to "", unlike_path(blog.id), method: :delete, remote: true, class: "star-rating-front" %>
      <%= blog.likes.count.to_s %>
    <% else %>
      <%= link_to "â˜…", like_path(blog.id), method: :post, remote: true, class: "star-rating-back" %>
      <%= blog.likes.count.to_s %>
    <%end%>
  </div>
</div>